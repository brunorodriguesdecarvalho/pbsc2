<!DOCTYPE html>
<html>
    <head>
        <title>Planejamento: Lista de Iniciativas</title>
        <%- include('meta'); %>        
        <link rel="stylesheet" type="text/css" href="../styles/main.css">
    </head>
    <body>
        <%- include('nav'); %>
        <main class="container mt-5">
            <br><br>
            <div class="container row mt-4 mb-1">
                <button class="btn btn-primary mr-1" type="button" data-toggle="collapse" data-target="#collapseConcluido" aria-expanded="false" aria-controls="collapseConcluido">
                    Exibir Iniciativas Encerradas
                </button>
                <button class="btn btn-primary" type="button" data-toggle="collapse" data-target="#collapseDemais" aria-expanded="false" aria-controls="collapseDemais">
                    Exibir Demais Iniciativas
                </button>
            </div>
            <div class="collapse" id="collapseConcluido">
                <div class="card card-body">
                    <div class="row" id="IniCompl"></div>
                </div>
            </div>
            <div class="collapse" id="collapseDemais">
                <div class="card card-body">
                    <div class="row" id="Ini"></div>
                </div>
            </div>
        </main>
    </body>
    <%- include('scripts'); %>
    <script src="../src/todasIniciativas.js" type="text/javascript"></script>
    <script src="../src/botao_excluir.js" type="text/javascript"></script>
    <script src="../src/botao_concluir.js" type="text/javascript"></script>
    <script src="../src/botao_andamento.js" type="text/javascript"></script>
    <script src="../src/emconstrucao.js" type="text/javascript"></script>
    <script src="../src/expandirCard.js" type="text/javascript"></script>
    <script src="../src/colorirStatus.js" type="text/javascript"></script>
    <script src="../src/ajustaDataBr.js" type="text/javascript"></script>
    <script src="../src/botao_editarPrazo.js" type="text/javascript"></script>
    <script src="../src/botao_editarStatus.js" type="text/javascript"></script>
    <script src="../src/botao_editarDetalhe.js"></script>
</html>

